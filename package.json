  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "apollo-server-koa": "^1.3.6",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.1",
    "config-lite": "^2.1.0",
    "dataloader": "^1.4.0",
    "form-data": "^2.1.4",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.0.4",
    "highlight.js": "^9.9.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-range": "^0.3.0",
    "koa-router": "7.0.0",
    "koa-static-cache": "^5.1.2",
    "lodash": "^4.17.10",
    "marked": "^0.4.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "nuxt-edge": "^2.0.0-25500158.1b46a95",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.10",
    "sqlite3": "^4.0.0",
    "typeorm": "^0.2.7",
    "upyun": "^2.0.4",
    "ylru": "^1.2.0",
    "yup": "^0.26.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w server -e ts,tsx --exec ts-node ./server/app.ts",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production tsc && node ./serverDist/app.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "NODE_ENV=test jest --forceExit",
    "gen-schema-types": "gql2ts server/graphql/schema.graphql -o server/types/schema.d.ts"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "roots": [
      "<rootDir>/server/tests"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.1",
    "@types/graphql": "^0.13.3",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-router": "^7.0.30",
    "@types/lodash": "^4.14.110",
    "@types/marked": "^0.4.0",
    "@types/md5": "^2.1.32",
    "@types/node": "^10.5.8",
    "@types/sequelize": "^4.27.23",
    "@types/sqlite3": "^3.1.3",
    "@types/yup": "^0.24.8",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "gql2ts": "^1.8.2",
    "graphql-request": "^1.8.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "koa-webpack": "^5.0.2",
    "koa2-connect-history-api-fallback": "0.0.6",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "prettier": "1.12.1",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
